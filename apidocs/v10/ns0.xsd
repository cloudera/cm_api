              <xs:documentation>
                <![CDATA[The time when HDFS usage info was last collected. No information
beyond this time can be provided.]]>
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="items" minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                <![CDATA[A list of per-user usage information at the requested
time granularity.]]>
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="reportRows" type="apiHdfsUsageReportRow" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                    <xs:documentation>
                      <![CDATA[A list of per-user usage information at the requested
time granularity.]]>
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
